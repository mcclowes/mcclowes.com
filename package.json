{
  "name": "mcclowes.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "prestart": "node scripts/generate-env.js",
    "start": "docusaurus start",
    "prebuild": "node scripts/generate-env.js",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "test": "node --require ./scripts/test-register.cjs --test",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "husky": {
    "hooks": {
      "pre-push": "command -v git-lfs >/dev/null 2>&1 && git lfs pre-push \"$@\" || true"
    }
  },
  "dependencies": {
    "@docusaurus/core": "^3.6.3",
    "@docusaurus/module-type-aliases": "^3.6.3",
    "@docusaurus/plugin-vercel-analytics": "^3.6.3",
    "@docusaurus/preset-classic": "^3.6.3",
    "@docusaurus/theme-live-codeblock": "^3.6.3",
    "@docusaurus/theme-mermaid": "^3.6.3",
    "@giscus/react": "^3.1.0",
    "@mdx-js/react": "^3.0.1",
    "clsx": "^2.1.1",
    "docusaurus-plugin-cookie-consent": "^3.1.0",
    "docusaurus-plugin-glossary": "^2.0.2",
    "docusaurus-plugin-image-zoom": "^2.0.0",
    "docusaurus-plugin-sass": "^0.2.6",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "posthog-js": "^1.264.2",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-masonry-css": "^1.0.16",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
